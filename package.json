{
  "name": "Homework-Manager",
  "version": "1.0.0",
  "description": "A Homework Manager",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && electron .",
    "package-mac": "npm run build && electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=build --asar",
    "package-win": "npm run build && electron-packager . homework-manager --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=build --version-string.CompanyName=Upsilon --version-string.FileDescription=\"Homework Manager\" --version-string.ProductName=\"Homework Manager\" --asar",
    "package-linux": "npm run build && electron-packager . homework-manager --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=build --asar",
    "package-all": "npm run package-mac && npm run package-win && npm run package-linux"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SCXRRADIATION/hm-man.git"
  },
  "keywords": [],
  "author": "Upsilon",
  "contributors": [
    "Ryan Bester (https://www.ryanbester.com)"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/SCXRRADIATION/hm-man/issues"
  },
  "homepage": "https://github.com/SCXRRADIATION/hm-man#readme",
  "devDependencies": {
    "electron": "^11.2.0",
    "electron-packager": "^15.2.0",
    "electron-window-state": "^5.0.3",
    "googleapis": "^39.2.0",
    "typescript": "^4.1.3"
  }
}
